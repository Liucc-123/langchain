    "    \"    \\\"\\\"\\\"运行测试\\\"\\\"\\\"\\n\",\n",
    "    \"    return \\\"Tests passed\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 创建带有 TodoListMiddleware 的 agent\\n\",\n",
    "    \"agent = create_agent(\\n\",\n",
    "    \"    model=\\\"gpt-5-mini\\\",\\n\",\n",
    "    \"    tools=[read_file, write_file, run_tests],\\n\",\n",
    "    \"    middleware=[TodoListMiddleware()]\\n\",\n",
    "    \")\\n\",\n",
